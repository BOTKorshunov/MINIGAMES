<Page x:Class="MINIGAMES.Games.Snake.Pages.SnakeWinPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MINIGAMES.Games.Snake.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SnakeWinPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBlock Text="ПОБЕДА!" FontSize="70" FontWeight="Bold"/>
            <Button Content="Вернуться в меню" Height="50" Margin="0, 20, 0, 0"
                    Name="btnBack" Click="btnBack_Click"/>
        </StackPanel>
    </Grid>
</Page>
